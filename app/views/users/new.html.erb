<h1> Sign up for Art Catalogue </h1>




<%= form_tag("/users") do %>
    <%= label_tag "Email:" %>
    <%= text_field_tag :email %>
    <br>
    <%= label_tag "Password:" %>
    <%= password_field_tag :password %>
    <br>

    <%= label_tag "Password Confirmation:" %>
    <%= password_field_tag :password %>


    <%= submit_tag "Sign up"%>
<% end %>