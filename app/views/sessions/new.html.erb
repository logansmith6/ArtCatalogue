<h1> Login to Art Catalogue </h1>
    <% if flash[:notice]%>
    <h2> <%= flash[:notice] %>%
    <% end %>



<%= form_tag("/sessions") do %>
    <%= label_tag "Email:" %>
    <%= text_field_tag :email %>

    <%= label_tag "Password:" %>
    <%= password_field_tag :password %>
    <br>



    <%= submit_tag "Login"%>
<% end %>