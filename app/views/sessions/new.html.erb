<pre>
<a href="/signup">Signup!</a>
<h1> Login to Art Catalogue </h1>
    



<%= form_tag("/sessions") do %>
    <%= label_tag "Email:" %>
    <%= text_field_tag :email %>

    <%= label_tag "Password:" %>
    <%= password_field_tag :password %>
    <br>
    <% if flash[:notice]%>
    <p style="color:red">
     <%= flash[:notice] %>
     </p>
    <% end %>

    <%= submit_tag "Login"%>
<% end %>

</pre>
